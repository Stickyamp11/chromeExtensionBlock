{
    "manifest_version": 2,
    "name": "PopUp blocker",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "storage",
      "webRequest",
      "webRequestBlocking",
      "*://*/*",
      "<all_urls>"
    ],
  "background": {
    "service_worker": "background.js"
  },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
  }